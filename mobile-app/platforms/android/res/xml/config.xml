<?xml version='1.0' encoding='utf-8'?>
<widget id="com.beta.college.Uguru" version="1.2.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="applicationPreferences">
        <param name="android-package" value="com.appgiraffe.plugins.applicationPreferences" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
