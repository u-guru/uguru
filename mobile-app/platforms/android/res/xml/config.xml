    </feature>
    <feature name="CardIO">
        <param name="android-package" value="com.keepe.plugins.cardio.CardIO" />
    </feature>
    <feature name="AppAvailability">
        <param name="android-package" value="com.ohh2ahh.appavailability.AppAvailability" />
    </feature>
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <feature name="DatePickerPlugin">
        <param name="android-package" value="com.plugin.datepicker.DatePickerPlugin" />
    </feature>
    <feature name="ImagePicker">
        <param name="android-package" value="com.synconset.ImagePicker" />
    </feature>
    <feature name="Clipboard">
        <param name="android-package" value="com.verso.cordova.clipboard.Clipboard" />
