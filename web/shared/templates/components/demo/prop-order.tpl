<div class="perspective-container full-xy flex-wrap-center bg-rocket overflow-auto no-scrollbar">
	<ul class="p15-grid full-x"
		init-with="p:[display:flex, flex-wrap:wrap]">
		<li init-with="p:[flex-basis:1, flex-grow:1, order:8]"
			on-init="p:[order:8:7:1000:easeOutSine]">
			<div class="full-x height-128 bg-auburn radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1, flex-grow:1, order:7]"
			on-init="p:[order:7:4:400:easeOutBounce]:delay-200">
			<div class="full-x height-128 bg-orange radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1, flex-grow:1, order:6]"
			on-init="p:[order:6:5:1000:easeOutSine]">
			<div class="full-x height-128 bg-gold radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1, flex-grow:1, order:5]"
			on-init="p:[order:5:4:1000:easeOutSine]">
			<div class="full-x height-128 bg-moola radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1, flex-grow:1, order:4]"
			on-init="p:[order:4:3:1000:easeOutSine]">
			<div class="full-x height-128 bg-azure radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1, flex-grow:1, order:3]"
			on-init="p:[order:3:2:1000:easeOutSine]">
			<div class="full-x height-128 bg-cobalt radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1, flex-grow:1, order:2]"
			on-init="p:[order:2:1:1000:easeOutSine]">
			<div class="full-x height-128 bg-eggplant radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1, flex-grow:1, order:1]"
			on-init="p:[order:1:8:1000:easeOutSine]">
			<div class="full-x height-128 bg-cerise radius-2"></div>
		</li>
	</ul>
</div>
