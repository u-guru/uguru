<div class="perspective-container full-xy flex-center-wrap bg-rocket">
	<ul class="flex-wrap p15-grid full-x"
		init-with="p:[transform:translateX(100%)]"
		on-init="p:[transform:translateX(100%):translateX(0%):1000:easeOutCirc]">
		<li init-with="p:[flex-basis:1,flex-grow:5]"
			on-init="p:[flex-grow:5:10:2000:easeOutBounce]">
			<div class="full-x height-96 bg-robin radius-2"
				on-init="p:[background:rgba(43#79#184#1):rgba(67#203#157#1):2000:easeOutSine]"></div>
		</li>
		<li init-with="p:[flex-basis:1,flex-grow:4]"
			on-init="p:[flex-grow:4:2:2000:easeOutBounce]">
			<div class="full-x height-96 bg-robin radius-2"
				on-init="p:[background:rgba(43#79#184#1):rgba(67#203#157#1):2000:easeOutSine]"></div>
		</li>
		<li init-with="p:[flex-basis:1,flex-grow:3]"
			on-init="p:[flex-grow:3:1.5:2000:easeOutBounce]">
			<div class="full-x height-96 bg-robin radius-2"
				on-init="p:[background:rgba(43#79#184#1):rgba(67#203#157#1):2000:easeOutSine]"></div>
		</li>
		<li init-with="p:[flex-basis:1,flex-grow:2]"
			on-init="p:[flex-grow:2:1:2000:easeOutBounce]">
			<div class="full-x height-96 bg-robin radius-2"
				on-init="p:[background:rgba(43#79#184#1):rgba(67#203#157#1):2000:easeOutSine]"></div>
		</li>
		<li init-with="p:[flex-basis:1,flex-grow:1]"
			on-init="p:[flex-grow:1:0.5:2000:easeOutBounce]">
			<div class="full-x height-96 bg-robin radius-2"
				on-init="p:[background:rgba(43#79#184#1):rgba(67#203#157#1):2000:easeOutSine]"></div>
		</li>
	</ul>

	<ul class="flex-wrap p15-grid full-x"
		init-with="p:[transform:translateX(100%)]"
