			<div class="full-x height-96 bg-robin radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1,flex-grow:1]"
			on-init="p:[flex-grow:1:0.5:1000:easeInOutCirc]">
			<div class="full-x height-96 bg-robin radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1,flex-grow:1]"
			on-init="p:[flex-grow:1:2:1000:easeInOutCirc]">
			<div class="full-x height-96 bg-robin radius-2"></div>
		</li>
		<li init-with="p:[flex-basis:1,flex-grow:1]"
			on-init="p:[flex-grow:1:0.5:1000:easeInOutCirc]">
			<div class="full-x height-96 bg-robin radius-2"></div>
		</li>
	</ul>

