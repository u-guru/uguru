<div class="full-xy flex-center-wrap">
	<ul class="p15-grid flex-wrap-center full-x">
		<li>
			<div class="perspective-container"
				init-default
				on-init="p:[-webkit-perspective-origin:50% 50%:-50%% 50%:1000:easeOutSine]">
				<div class="bg-robin radius-2 flex-center semibold" style="width: 300px; height: 300px;"
					init-with="p:[tr:rotateX(10deg) rotateY(10deg), tro:bottom left]">easeOutSine</div>
			</div>
		</li>
		<li>
			<div class="perspective-container"
				init-default
				on-init="p:[-webkit-perspective-origin:50% 50%:-50%% 50%:1000:easeOutBounce]">
				<div class="bg-robin radius-2 flex-center semibold" style="width: 300px; height: 300px;"
					init-with="p:[tr:rotateX(10deg) rotateY(10deg), tro:bottom left]">easeOutBounce</div>
			</div>
		</li>
		<li>
			<div class="perspective-container"
				init-default
				on-init="p:[-webkit-perspective-origin:50% 50%:-50%% 50%:1000:easeOutCirc]">
				<div class="bg-robin radius-2 flex-center semibold" style="width: 300px; height: 300px;"
					init-with="p:[tr:rotateX(10deg) rotateY(10deg), tro:bottom left]">easeOutCirc</div>
			</div>
		</li>
		<li>
			<div class="perspective-container"
				init-default
				on-init="p:[-webkit-perspective-origin:50% 50%:-50%% 50%:1000:elastic]">
				<div class="bg-robin radius-2 flex-center semibold" style="width: 300px; height: 300px;"
					init-with="p:[tr:rotateX(10deg) rotateY(10deg), tro:bottom left]">elastic</div>
			</div>
		</li>
	</ul>
</div>
