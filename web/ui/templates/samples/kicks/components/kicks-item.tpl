<div rad="2px" hide-x hide-y>
	<h1 height="56px" p="6px" y-center x-center bg="white-25p" f-s="16px" f-w="600" color="white" caps t-a="center" class="caps txt-white txt-center">{{shoes.shoe_name}}</h1>
	<div bg="white">
		<figure class="txt-slate">
			{{shoes.release_year}}
			{{shoes.release_day}}
			{{shoes.shoe_url}}
			{{shoes.release_month}}
			{{shoes.shoe_price}}
			{{shoes.shoe_img_url}}
			{{shoes.shoe_name}}
		</figure>
		<rating-stars custom class="rating-stars" data-rating=""></rating-stars>
	</div>
	<hr m="0" border="0" bg="sienna" width="100" height="5px"/>
	<inspect-data/>
</div>
