{
    "vars": {
        "components": {
            "columns": ["Html", "output"]
        },
        "examples": {
            "list": [
                {
                    "title": "List o' numbers",
                    "html": "<div u-list='num in [1,2,3,4,5]' x-center y-center w='48px' h='48px' m='6px' bg='sienna' border='2px solid white' rad='2px' f-s='24px' f-w='600'>{{num}}</div>"
                },
                {
                    "title": "List o' strings",
                    "html": "<div u-list=\"person in ['sam', 'nate', 'mary', 'kate']\" x-center y-center p-x='12px' h='48px' m='6px' bg='sienna' border='2px solid white' rad='2px' f-s='24px' f-w='600'>{{person}}</div>"
                },
                {
                    "title": "List o' dictionaries",
                    "html": "<div u-list='rgba_item in [{rgb:\"255, 255, 255\", opacity: 0.5}, {rgb:\"0, 0, 0\", opacity: 0.8}]' x-center y-center h='48px' p-x='12px' m='6px' bg='sienna' border='2px solid white' rad='2px' f-s='24px' f-w='600'  y='center' width='100'> <div stretch  grow='3' width='50px' style='background-color:rgba({{rgba_item.rgb}},{{rgba_item.opacity}})'>&nbsp;&nbsp;&nbsp;&nbsp;</div><div width='75%' p='6px' f-s='12px'>rgb: {{rgba_item.rgb}}, <br>opacity: {{rgba_item.opacity}}</div></div>"
                }

            ]
        }
    },
    "config": {
        "pathParams": {},
        "base_url": "^",
        "parse": {
            "float": ["content.track.duration"]
        },
        "scripts": {
            "base": "https://",
            "queue": [
                {
                    "path": "s3-us-west-1.amazonaws.com/ui-guru/samples/soundcloud.json",
                    "mappings": [
                    {
                        "from": "collection[1].track",
                        "to":"content.track"
                    }
                    ]
                },
                {
                    "path": "wis.sndcdn.com/xeXS8StuaDqa_m.json",
                    "mappings": [
                        {
                            "to":"content.viz"
                        }
                    ]
                }
            ]
        }
    },
    "content": {
        "client": {
            "setup": {},
            "components": {}
        },
        "examples": [{
                    "html": "<div u-list='rgba_item in [{rgb:(255, 255, 255), opacity: 0.5}]>"
                }]
    },
    "components": {
        "list-item-example-one": {
            "template_url": "./ui/templates/concepts/list/components/list-item-example.tpl",
